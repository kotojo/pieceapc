<div class="page-header">
  <h1>Nuh uh buddy, only you here.</h1>
</div>

<div class="table-responsive flat">
  <table class="table table-hover">
    <thead>
      <tr class="headingTr">
        <th>First name</th>
        <th>Last name</th>
        <th>Email</th>
      </tr>
    </thead>

    <tbody>
      <%= content_tag_for(:tr, current_user) do |user| %>
        <td><%= user.first_name %></td>
        <td><%= user.last_name %></td>
        <td><%= user.email %></td>
      <% end %>
    </tbody>
  </table>
</div>

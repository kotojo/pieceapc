<% @arr.each do |i| %>
  <div class="col-sm-6 items">
    <%= image_tag i[0]["Image"]["SmallImagePath"], class: "text-center" %><br>
    <%= i[0]["Title"] %><br>
    Model: <%= i[0]["Model"] %><br>
    Price: <%= i[0]["FinalPrice"] %><br>
    <% count = 0 %>
    <%= link_to 'Destroy', @computer.parts[count], method: :delete %><br>
  </div>
<% end %>
